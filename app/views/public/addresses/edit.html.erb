<h3>配送先編集</h3>

<%= form_with model: @address do |f| %>
  <table>
    <tr>
      <td>郵便番号（ハイフンなし）</td>
      <td><%= f.text_field :postal_code %></td>
    </tr>
    <tr>
      <td>住所</td>
      <td><%= f.text_field :address %></td>
    </tr>
    <tr>
      <td>宛名</td>
      <td><%= f.text_field :name %></td>
    </tr>
  </table>
  <%= f.submit '変更内容を保存' %>
  <% end %>